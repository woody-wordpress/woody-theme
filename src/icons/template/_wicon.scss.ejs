@font-face {
    font-family: "<%= fontname %>";
    font-display: swap;
    src: url('<%= fontPath %><%= fontname %>.eot?t=<%= timestamp %>'); /* IE9*/
    src: url('<%= fontPath %><%= fontname %>.eot?t=<%= timestamp %>#iefix') format('embedded-opentype'), /* IE6-IE8 */
    url("<%= fontPath %><%= fontname %>.woff2?t=<%= timestamp %>") format("woff2"),
    url("<%= fontPath %><%= fontname %>.woff?t=<%= timestamp %>") format("woff"),
    url('<%= fontPath %><%= fontname %>.ttf?t=<%= timestamp %>') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
    url('<%= fontPath %><%= fontname %>.svg?t=<%= timestamp %>#<%= fontname %>') format('svg'); /* iOS 4.1- */
}

.wicon {
    @include wicon-styles;
}


@each $icon, $char in $wicons {
    .#{$icon}:before {
        content: $char;
    }
}
